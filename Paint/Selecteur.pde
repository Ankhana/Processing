void selecteur(){
  //sélecteur de couleur
  fill(255);
  rect(5,5,width-10,60);
  //rouge
  rectangle(10,#F70A0A);
  //orange
  rectangle(70,#E88317);
  //jaune
  rectangle(130,#E1FF05);
  //vert
  rectangle(190,#30FF03);
  //cyan
  rectangle(250,#03FFE8);
  //bleu foncé
  rectangle(310,#0343FF);
  //violet
  rectangle(370,#9D03FF);
  //rose
  rectangle(430,#F264D8);
  //noir
  rectangle (490,#363434);
  
}

